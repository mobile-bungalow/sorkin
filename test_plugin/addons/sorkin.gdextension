[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.5
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../target/debug/libsorkin.so"
linux.release.x86_64 =   "res://../target/release/libsorkin.so"
windows.debug.x86_64 =   "res://../target/debug/sorkin.dll"
windows.release.x86_64 = "res://../target/release/sorkin.dll"
macos.debug =            "res://../target/debug/libsorkin.dylib"
macos.release =          "res://../target/release/libsorkin.dylib"
macos.debug.arm64 =      "res://../target/debug/libsorkin.dylib"
macos.release.arm64 =    "res://../target/release/libsorkin.dylib"
